{
    "name": "99designs/twirfony",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Twirfony\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "example/Haberdasher/src"
        },
