        "google/protobuf": "^3",
        "psr/http-message": "^1",
        "guzzlehttp/guzzle": "^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/phpunit-bridge": "^6.1"
    }
}
